    "@nestjs/axios": "^1.0.0",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/typeorm": "7.1.5",
    "@types/bcrypt": "^5.0.0",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^8.3.4",
    "apollo-server-express": "^3.10.3",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "12.0.0",
    "jsonwebtoken": "^8.5.1",
    "nestjs-admin": "^0.4.1",
    "nestjs-pino": "^3.1.1",
